{
  "name": "oscillator_2cycle",
  "description": "Two internal bits that toggle each other in a cycle",
  "initial_state": {
    "inputs": [],
    "outputs": [0],
    "internals": [1, 0]
  },
  "test_cases": [
    {
      "description": "Oscillation cycle starting from internal[0]=1, internal[1]=0",
      "input_changes": {},
      "expected_ticks": [
        {
          "tick": 1,
          "state": {
            "inputs": [],
            "outputs": [1],
            "internals": [1, 1]
          },
          "changes": {
            "outputs": [1],
            "internals": [null, 1]
          }
        },
        {
          "tick": 2,
          "state": {
            "inputs": [],
            "outputs": [1],
            "internals": [0, 1]
          },
          "changes": {
            "internals": [0, null]
          }
        },
        {
          "tick": 3,
          "state": {
            "inputs": [],
            "outputs": [0],
            "internals": [0, 0]
          },
          "changes": {
            "outputs": [0],
            "internals": [null, 0]
          }
        },
        {
          "tick": 4,
          "state": {
            "inputs": [],
            "outputs": [0],
            "internals": [0, 0]
          },
          "changes": {}
        }
      ]
    }
  ]
}